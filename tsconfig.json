{
  "compilerOptions": {
    "target": "ESNext",
    "module": "CommonJS",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "lib": ["esnext"],
    "rootDir": ".",
    "baseUrl": ".",
    "outDir": "./.nexus",
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "sourceMap": true
  },
  "exclude": ["node_modules", "./.nexus"]
}
